{
    "name": "trifs/di-server",
    "description": "minimalistic framework on top of DI",
    "require": {
        "trifs/di": "1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "trifs/php-shirt": "dev-master",
        "sebastian/phpcpd": "*",
        "phpmd/phpmd": "2.*",
        "squizlabs/php_codesniffer": "2.*",
        "jakub-onderka/php-parallel-lint": "dev-master"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Dominik Znidar",
            "email": "dominik.znidar@3fs.si"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "trifs\\DIServer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "trifs\\DIServer\\Tests\\Unit\\": "tests/unit/DIServer"
        }
    },
    "repositories": [
        {
            "url": "https://github.com/3fs/php-shirt.git",
            "type": "git"
        }
    ]
}
